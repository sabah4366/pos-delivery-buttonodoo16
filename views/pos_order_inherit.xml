<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="view_so_form_inherit" model="ir.ui.view">
		<field name="name">pos.order.inherit</field>
		<field name="model">pos.order</field>
		<field name="inherit_id" ref="point_of_sale.view_pos_pos_form"/>
		<field name="arch" type="xml">
			<notebook position="inside">
				<page string="Delivery Details" >
					<form >
						  <group>
							  <field name="pos_country" readonly="1"/>
							   <field name="pos_delivery_type" readonly="1"/>
							   <field name="exp_delivery_date" readonly="1"/>
						  </group>

					</form>
				</page>
			</notebook>




		</field>
	</record>
</odoo>
